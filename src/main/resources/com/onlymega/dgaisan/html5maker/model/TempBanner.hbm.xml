<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.onlymega.dgaisan.html5maker.model.TempBanner" table="temp_banners">
		<id name="bannerId" type="java.lang.Integer">
			<column name="banner_id" />
			<generator class="increment" />
		</id>

		<property name="dataToken" type="string">
			<column name="data_token" />
		</property>

		<property name="bannerConfig" type="string">
			<column name="banner_config" />
		</property>

		<property name="htmlCode" type="string">
			<column name="html_code" />
		</property>
		
		<property name="imagesArray" type="string">
			<column name="images_array" />
		</property>

		<property name="bannerWidth" type="java.lang.Integer">
			<column name="banner_width" />
		</property>
		
		<property name="bannerHeight" type="java.lang.Integer">
			<column name="banner_height" />
		</property>
		
		<property name="bannerSize" type="java.lang.Integer">
			<column name="banner_size" />
		</property>

		<property name="dateCreated" type="java.util.Date">
            <column name="date_created" />
        </property>

	</class>
</hibernate-mapping>